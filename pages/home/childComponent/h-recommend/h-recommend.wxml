<!--pages/home/childComponent/w-recommend/w-recommend.wxml-->
<view class="recommends">
  <block wx:for="{{recommends}}" wx:key="item" >
    <view class="recommends-item">
      <image src="{{item.image}}"
             bindload="handlerImageLoad"></image>
      <view class="recommends-title"><text>{{item.title}}</text></view>
     
    </view>
  </block>
 </view>
